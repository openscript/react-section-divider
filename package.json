{
  "name": "react-section-dividers",
  "author": "openscript",
  "description": "A collection of React section dividers.",
  "keywords": [
    "react-component",
    "typescript",
    "react"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://openscript.github.io/react-section-dividers",
  "repository": "https://github.com/openscript/react-section-dividers",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "start:storybook": "start-storybook -s ./docs -p 6006",
    "build": "tsdx build",
    "build:storybook": "build-storybook -s ./docs --quiet",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src test stories",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/react-section-dividers.esm.js",
  "size-limit": [
    {
      "path": "dist/react-section-dividers.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-section-dividers.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@size-limit/preset-small-lib": "^4.7.0",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-storysource": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-loader": "^8.2.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}
